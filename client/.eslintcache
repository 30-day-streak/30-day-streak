)\n      .then(() => {\n        // reinitialise state\n        this.setState({\n          name: '',\n          description: '',\n          category: '',\n          url: ''\n        })\n      })\n      .catch(err => console.log(err));\n  }\n\n  render() {\n    return (\n      <div>\n        <h2>Create a new award</h2>\n        <p>Need some ideas for incentives to achieving your goals?</p>\n        <p>Check out what <a href=\"#\">other users have added</a> or see if there is something you like on <a href=\"https://www.developgoodhabits.com/reward-yourself/\" target=\"_blank\">this list</a> or <a href=\"https://organisemyhouse.com/reward-yourself/\" target=\"_blank\">this one</a>.</p>\n        <br />\n        <form onSubmit={this.handleSubmit}>\n\n          <label htmlFor=\"name\">Name</label>\n          <input\n            type=\"text\"\n            id=\"name\"\n            name=\"name\"\n            value={this.state.name}\n            placeholder=\"Give your reward a name\"\n            onChange={this.handleChange}\n          />\n          <br />\n\n          <label htmlFor=\"description\">Description</label>\n          <input\n            type=\"text\"\n            id=\"description\"\n            name=\"description\"\n            value={this.state.description}\n            placeholder=\"Add a more detailed description if you need one\"\n            onChange={this.handleChange} />\n          <br />\n\n          <label htmlFor=\"category\">Category</label>\n          <select\n            id=\"category\"\n            name=\"category\"\n            value={this.state.category}\n            onChange={this.handleChange}>\n            <option value=\"Food\">Food</option>\n            <option value=\"Activity/Experience\">Activity/Experience</option>\n            <option value=\"\" selected=\"selected\">Select a category</option>\n            <option value=\"Gift\">Gift</option>\n            <option value=\"Self-care\">Self-care</option>\n            <option value=\"Other\">Other</option>\n          </select>\n          <br />\n\n          <label htmlFor=\"url\">Link</label>\n          <input\n            type=\"text\"\n            id=\"url\"\n            name=\"url\"\n            value={this.state.url}\n            placeholder=\"You can add a link to an internet page here\"\n            onChange={this.handleChange} />\n          <br />\n\n          <button type=\"submit\">Save to my list of rewards</button>\n        </form>\n\n      </div >\n    )\n  }\n}\n","/Users/katherinegracey/OneDrive/IronHack/30-day-streak/client/src/services/auth.js",[],"/Users/katherinegracey/OneDrive/IronHack/30-day-streak/client/src/components/OneReward.js",["67","68"],"import React, { Component } from 'react'\nimport axios from 'axios';\n\nexport default function OneReward(props) {\n  return (\n    <div>\n      {props.rewards.map(reward => {\n        console.log('reward', reward)\n        return (\n          <div className=\"card\" key={ reward._id }>\n            <div className=\"card-header\">\n              <p>{ reward.category }</p>\n            </div>\n            <h2>{ reward.title }</h2>\n            <p><strong>Goal:</strong> { reward.goal }</p>\n            <p><strong>Dailt Target:</strong> { reward.dailyTarget.description } { reward.dailyTarget.number } { reward.dailyTarget.unit }</p>\n          </div>\n        )\n      })}\n    </div>\n  )\n}","/Users/katherinegracey/OneDrive/IronHack/30-day-streak/client/src/components/OneChallenge.js",[],{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":3,"column":8,"nodeType":"75","messageId":"76","endLine":3,"endColumn":18},{"ruleId":"77","severity":1,"message":"78","line":49,"column":27,"nodeType":"79","endLine":49,"endColumn":39},{"ruleId":"80","severity":1,"message":"81","line":49,"column":168,"nodeType":"82","endLine":49,"endColumn":183},{"ruleId":"80","severity":1,"message":"81","line":49,"column":256,"nodeType":"82","endLine":49,"endCo